 <xmodel version="2">
 <name>Test Model - Xparser 0.17.1 function conditions</name>
 <version>Version 1.0, 16.01.2018</version>
 <description>Author: Sander van der Hoog (svdhoog@gmail.com)</description>
 <models/>
  <environment>
  <functionFiles>
   <file>agent_functions.c</file>
  </functionFiles>
  <timeUnits>
   <timeUnit>
    <name>even</name>
    <unit>iteration</unit>
    <period>2</period>
   </timeUnit>
  </timeUnits>
  </environment>
  <agents>
  <xagent>
   <name>Agent</name>
   <description></description>
   <memory>
    <variable>
     <type>int</type>
     <name>id</name>
     <description>Agent ID.</description>
    </variable>
   </memory>
    <functions>
    <function>
     <name>is_true</name>
     <description></description>
     <currentState>start_Agent</currentState>
     <nextState>end_Agent</nextState>
     <condition>
	 <lhs>	 
		 <time>
		 <period>even</period>
		 <phase>1</phase>
		 </time>
	</lhs>
	<op>OR</op>
	<rhs>
    <not>
		<lhs>
		  <value>a.id</value>
		</lhs>
		<op>EQ</op>
		<rhs>
		  <value>1</value>
		</rhs>
    </not>
	 </rhs>
	 </condition>
     <inputs/>
     <outputs/>
    </function>
    <function>
     <name>is_false</name>
     <description></description>
     <currentState>start_Agent</currentState>
     <nextState>end_Agent</nextState>
     <condition>
	<not>
	 <lhs>	 
		 <time>
		 <period>even</period>
		 <phase>1</phase>
		 </time>
	</lhs>
	<op>OR</op>
	<rhs>
    <not>
		<lhs>
		  <value>a.id</value>
		</lhs>
		<op>EQ</op>
		<rhs>
		  <value>1</value>
		</rhs>
    </not>
	 </rhs>
	</not>
	 </condition>
     <inputs/>
     <outputs/>
    </function>
   </functions>
  </xagent>
 </agents>
 <messages/>
</xmodel>